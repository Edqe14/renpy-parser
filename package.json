{
  "name": "renpy-parser",
  "version": "1.0.0",
  "description": "Parse .rpy files to JSON files",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --verbose --detectOpenHandles",
    "verify": "tsc -p ./tsconfig.json --noEmit",
    "build": "tsc -p ./tsconfig.json && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "docs": "typedoc"
  },
  "keywords": [
    "renpy",
    "parser",
    "rpy"
  ],
  "author": "Edqe_",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "lexure": "0.17.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.3.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-plugin": "4.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.3",
    "prettier": "2.5.0",
    "ts-jest": "27.0.7",
    "tscpaths": "0.0.9",
    "typedoc": "0.22.10",
    "typescript": "4.5.2"
  }
}
